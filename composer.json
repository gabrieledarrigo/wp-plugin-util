{
    "name": "darrigo/wp-plugin-utils",
    "description": "An object oriented package to be reused in my WordPress plugin",
    "type": "library",
    "keywords": ["wordpress", "plugin", "php"],
    "homepage": "https://github.com/gabrieledarrigo/wp-plugin-utils",
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriele D'Arrigo",
            "email": "darrigo.g@gmail.com"
        }
    ],
    "minimum-stability": "alpha",
    "autoload": {
        "psr-4": {
            "Darrigo\\WpPluginUtils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Darrigo\\WpPluginUtils\\Tests\\": "tests/"
        }
    },
    "require": {
        "doctrine/collections": "^1.3"
    }
}
